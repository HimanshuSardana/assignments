#include <bits/stdc++.h>

using namespace std;
        
int main() {
        int max_weight = 50;
        vector<int> weights = {10, 20, 30};
        vector<int> profits = {60, 100, 120};

        int n = weights.size();

        vector<int> ans(n);

        int current_weight = 0;
        while(current_weight < max_weight) {
                int highestWeight = max(weights);
                int highestWeightIndex = find(weights.begin(), weights.end(), highestWeight) - weights.begin();
        }

}
